# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Apple Developer Configuration for Wallet
# These values come from your Apple Developer account
VITE_APPLE_TEAM_ID=YOUR_TEAM_ID
VITE_APPLE_PASS_TYPE_ID=pass.com.yourcompany.loyalty
VITE_APPLE_KEY_PASSPHRASE=your_certificate_passphrase

# Apple Certificates (Base64 encoded)
# You need to encode your certificates in Base64 format
# Example: base64 -i wwdr.pem -o wwdr.txt
VITE_APPLE_WWDR_CERT=base64_encoded_wwdr_certificate
VITE_APPLE_SIGNER_CERT=base64_encoded_signer_certificate
VITE_APPLE_SIGNER_KEY=base64_encoded_signer_key